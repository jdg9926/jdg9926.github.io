{
  "name": "myresume",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://jdg9926.github.io",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "shx cp build/index.html build/404.html",
    "build:docs": "npm run build && rimraf docs && shx cp -r build/* docs",
    "deploy": "npm run build:docs && git add docs && git commit -m 'deploy: update docs' || echo 'no changes' && git push",
    "clean": "rimraf node_modules build docs",
    "lint": "eslint --ext .js src",
    "test": "react-scripts test --watchAll=false"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "dompurify": "^3.2.6",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "eslint": "^9.9.0",
    "eslint-config-react-app": "^7.0.1",
    "rimraf": "^6.0.1",
    "shx": "^0.3.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
